{
  "resources": [
    {
      "path": "data.csv",
      "name": "data",
      "profile": "tabular-data-resource",
      "scheme": "file",
      "format": "csv",
      "hashing": "md5",
      "encoding": "utf-8",
      "schema": {
        "fields": [
          {
            "name": "Year",
            "type": "integer",
            "title": "Année"
          },
          {
            "name": "Sex",
            "type": "string",
            "constraints": {
              "enum": [
                "Les deux sexes",
                "Homme",
                "Femme",
                ""
              ]
            },
            "title": "Sexe"
          },
          {
            "name": "Age Group",
            "type": "string",
            "constraints": {
              "enum": [
                "15-19 years-old",
                "20-24 years-old",
                "25+ years-old",
                "25-44 years-old",
                "45+ years-old",
                "Tous les groupes d’âge",
                ""
              ]
            },
            "title": "Groupe d’âge"
          },
          {
            "name": "Tobacco Product",
            "type": "string",
            "constraints": {
              "enum": [
                "Tout produit du tabac",
                "Cigarettes",
                "Cigares",
                "Cigarettes électroniques",
                "Petits cigares ou cigarillos",
                "Tabac sans fumée",
                "Vapotage",
                "Pipe à eau avec du tabac",
                "Pipe",
                "Tabac à chiquer",
                ""
              ]
            },
            "title": "Produits du tabac"
          },
          {
            "name": "Value",
            "type": "number",
            "title": "Valeur"
          }
        ]
      }
    }
  ],
  "profile": "data-package",
  "name": "3-a-1",
  "title": "Prévalence de la consommation actuelle de tabac chez les personnes de 15 ans ou plus (taux comparatifs par âge)"
}